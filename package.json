{
  "name": "typeorm-factory",
  "version": "0.0.10",
  "description": "A factory library for typeorm",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "repository": "https://github.com/antonversal/typeorm-factory.git",
  "bugs": {
    "url": "https://github.com/antonversal/typeorm-factory/issues"
  },
  "author": "Anton Versal <anton.versal@gmail.com>",
  "keywords": [
    "typeorm",
    "factory",
    "test",
    "tests",
    "testing",
    "unit",
    "typescript",
    "javascript",
    "orm"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "tslint -p .",
    "build": "yarn tsc -p tsconfigBuild.json",
    "test:ci": "jest --ci --testResultsProcessor=\"./node_modules/jest-junit-reporter\" --coverage",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "prepare": "yarn build",
    "precommit": "tsc --noEmit && lint-staged"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/",
      "/support/"
    ]
  },
  "lint-staged": {
    "server/**/*.ts": [
      "yarn lint --fix",
      "git add"
    ],
    "*.{js,ts,md}": [
      "yarn prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "typeorm": "^0.1.6"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "coveralls": "^3.0.2",
    "husky": "^0.14.3",
    "jest-cli": "^23.6.0",
    "jest-junit-reporter": "^1.1.0",
    "lint-staged": "^7.2.2",
    "pg": "^7.4.3",
    "prettier": "^1.14.2",
    "reflect-metadata": "^0.1.12",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typeorm": "^0.2.7",
    "typescript": "^3.0.3"
  }
}
